# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# OpenWeather API (for location services)
VITE_OPENWEATHER_API_KEY=your_openweather_api_key

# Pusher (for real-time features)
VITE_PUSHER_KEY=your_pusher_key
VITE_PUSHER_CLUSTER=your_pusher_cluster
VITE_PUSHER_INSTANCE_ID=your_pusher_instance_id

# Stripe (for payments)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Yelp API (for restaurant search)
# Get your API key from: https://www.yelp.com/developers/v3/manage_app
# Note: You need to create a Yelp Developer account and create an app to get these keys
VITE_YELP_API_KEY=your_yelp_api_key
# This should be the same key, used by the Netlify function (no VITE_ prefix)
YELP_API_KEY=your_yelp_api_key

# OpenAI API (for AI features)
OPENAI_API_KEY=your_openai_api_key

# Pica Integration (for AI services)
PICA_SECRET_KEY=your_pica_secret_key
PICA_OPENAI_CONNECTION_KEY=your_pica_openai_connection_key